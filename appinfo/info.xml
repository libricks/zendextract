<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>zendextract</id>
    <name>Zendesk Xtractor</name>
    <summary>Interface de communication avec ZenDesk</summary>
    <description><![CDATA[Cette application permet de se connecter à l&#39;API REST de ZenDesk afin de générer une extractions des tickets d&#39;assistance de la plateforme dans un fichier téléchargeable au format CSV (Comma-separated values).]]></description>
    <version>0.53</version>
    <licence>AGPL</licence>
    <author mail="contact@libricks.fr" homepage="http://www.libricks.fr/">Marc-Henri Pamiseux</author>
    <namespace>ZendExtract</namespace>

	<types>
		<filesystem/>
	</types>

    <documentation>
        <user>https://docs.nextcloud.org</user>
        <admin>https://docs.nextcloud.org</admin>
        <developer>https://docs.nextcloud.org</developer>
    </documentation>

    <category>tools</category>

    <website>https://github.com/libricks/zendextract/wiki</website>
    <bugs>https://github.com/libricks/zendextract/issues</bugs>
    <repository type="git">https://github.com/libricks/zendextract</repository>

    <ocsid></ocsid>
    <dependencies>
        <owncloud min-version="9.2"/>
        <nextcloud min-version="11"/>
    </dependencies>

    <settings>
        <admin>OCA\ZendExtract\Settings\Admin</admin>
    </settings>

	<navigations>
		<navigation>
			<id>zendextract</id>
			<name>Zend Extract</name>
			<route>zendextract.page.index</route>
			<icon>app.svg</icon>
			<type>link</type>
		</navigation>
	</navigations>
</info>
